{
  "name": "@vrooli/jobs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "pnpm build",
    "start": "pnpm node dist/index.js",
    "pretest": "rimraf dist && pnpm --filter @vrooli/shared build",
    "build-tests": "NODE_ENV=test swc src -d dist --config-file ../../.swcrc",
    "test": "pnpm build-tests && dotenv -e ../../.env-test -- mocha --file dist/__test/setup.js \"dist/**/*.test.js\"",
    "test-watch": "pnpm build-tests && dotenv -e ../../.env-test -- mocha --watch --file dist/__test/setup.js \"dist/**/*.test.js\""
  },
  "dependencies": {
    "@vrooli/prisma": "workspace:*",
    "@vrooli/redis": "workspace:*",
    "@vrooli/shared": "workspace:*",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.4.5"
  }
}
