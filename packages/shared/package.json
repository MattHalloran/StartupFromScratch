{
  "name": "@vrooli/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "pretest": "rimraf dist",
    "build-tests": "NODE_ENV=test swc src -d dist --config-file ../../.swcrc",
    "test": "pnpm build-tests && dotenv -e ../../.env-test -- mocha --file dist/__test/setup.js \"dist/**/*.test.js\"",
    "test-watch": "pnpm build-tests && dotenv -e ../../.env-test -- mocha --watch --file dist/__test/setup.js \"dist/**/*.test.js\""
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "5.4.5"
  }
}
