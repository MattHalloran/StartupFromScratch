  },
  "dependencies": {
    "@mui/material": "^5.15.1",
    "@mui/styles": "^5.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.18",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5",
    "vite": "^5.1.0"
  }
}
