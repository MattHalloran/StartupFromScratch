{
  "name": "@vrooli/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build:shared": "pnpm --filter @vrooli/shared run build",
    "build:compile": "vite build",
    "build": "run-s clean build:compile",
    "preview": "vite preview",
    "pretest": "rimraf dist",
    "test:compile": "swc src -d dist --source-maps",
    "test:build": "cross-env NODE_ENV=test run-s clean test:compile copy",
    "test:run": "dotenv -e ../../.env-test -- mocha --file dist/__test/setup.js dist/**/*.test.js",
    "test:watch": "dotenv -e ../../.env-test -- mocha --watch --file dist/__test/setup.js dist/**/*.test.js",
    "test": "run-s build:shared test:build test:run",
    "test-watch": "run-s build:shared test:build test:watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit"
  },
  "dependencies": {
    "@mui/material": "^5.15.1",
    "@mui/styles": "^5.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.18",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5",
    "vite": "^5.1.0"
  }
}
