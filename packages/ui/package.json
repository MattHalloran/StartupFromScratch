{
    "name": "@vrooli/ui",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "copy": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.md\" dist",
        "dev": "vite",
        "build:shared": "pnpm --filter @vrooli/shared run build",
        "build:compile": "vite build",
        "build": "run-s clean build:compile",
        "preview": "vite preview",
        "pretest": "rimraf dist",
        "start:development": "vite",
        "start:production": "serve --config serve.json",
        "test:compile": "swc src -d dist --source-maps",
        "test:build": "cross-env NODE_ENV=test run-s clean test:compile copy",
        "test:run": "dotenv -e ../../.env-test -- mocha --file dist/__test/setup.js dist/**/*.test.js",
        "test:watch": "dotenv -e ../../.env-test -- mocha --watch --file dist/__test/setup.js dist/**/*.test.js",
        "test": "run-s build:shared test:build test:run",
        "test-watch": "run-s build:shared test:build test:watch",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit"
    },
    "dependencies": {
        "@mui/material": "^5.15.1",
        "@mui/styles": "^5.15.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "1.3.0",
        "react-router-dom": "^6.11.2"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^8.6.4",
        "@storybook/addon-essentials": "^8.6.0",
        "@storybook/addon-interactions": "^8.6.0",
        "@storybook/addon-onboarding": "^8.6.0",
        "@storybook/blocks": "^8.6.0",
        "@storybook/react": "^8.6.0",
        "@storybook/react-vite": "^8.6.0",
        "@storybook/test": "^8.6.0",
        "@testing-library/react": "^14.1.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/chai": "^5.0.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "^18.16.18",
        "@types/react": "^18.0.29",
        "@types/react-dom": "^18.0.11",
        "@types/sinon": "^17.0.3",
        "@vitejs/plugin-react": "^4.0.0",
        "chai": "^5.1.2",
        "cpy-cli": "^5.0.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^8.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-perf": "^3.3.3",
        "eslint-plugin-storybook": "^0.12.0",
        "jsdom": "^26.0.0",
        "mocha": "^10.2.0",
        "msw": "^2.7.3",
        "msw-storybook-addon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.10",
        "sinon": "^19.0.2",
        "storybook": "^8.6.0",
        "ts-node": "^10.9.1",
        "typescript": "5.4.5",
        "vite": "^5.4.7",
        "why-is-node-running": "^2.3.0"
    },
    "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}