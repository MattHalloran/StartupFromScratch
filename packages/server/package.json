{
  "name": "@vrooli/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "pnpm build",
    "start": "pnpm node dist/index.js",
    "test": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha --require ts-node/register --file src/__test/setup.ts \"src/__test/**/*.ts\"",
    "test-watch": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha --watch --require ts-node/register --file src/__test/setup.ts \"src/__test/**/*.ts\""
  },
  "dependencies": {
    "@vrooli/prisma": "workspace:*",
    "@vrooli/redis": "workspace:*",
    "@vrooli/shared": "workspace:*",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.14.1",
    "@types/supertest": "^2.0.12",
    "chai": "^5.0.0",
    "concurrently": "^8.0.1",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.4.5"
  }
}
